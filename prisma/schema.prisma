generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int       @id @default(autoincrement())
  name      String?
  email     String    @unique
  createdAt DateTime  @default(now())
  projects  Project[]
}

model Project {
  id           Int      @id @default(autoincrement())
  name         String
  createdAt    DateTime @default(now())
  lastModified DateTime @default(now())
  stateCode    String?
  viewCode     String?
  staticCode   String?
  ownerId      Int
  owner        User     @relation(fields: [ownerId], references: [id])
}
