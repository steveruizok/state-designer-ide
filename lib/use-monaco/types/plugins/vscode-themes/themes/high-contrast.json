        {
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#b5cea8"
            }
        },
        {
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#ce9178"
            }
        },
        {
            "scope": "markup.changed",
            "settings": {
                "foreground": "#569cd6"
            }
        },
        {
            "scope": "meta.selector",
            "settings": {
                "foreground": "#d7ba7d"
            }
